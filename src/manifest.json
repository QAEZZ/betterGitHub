{

  "manifest_version": 3,
  "name": "BetterGitHub",
  "version": "1.0",

  "description": "Changes the appearence of GitHub.",

  "icons": {
    "16": "icons/icon.jpg",
    "32": "icons/icon.jpg",
    "48": "icons/icon.jpg",
    "128": "icons/icon.jpg"
  },

  "content_scripts": [
    {
      "matches": ["*://*.github.com/*"],
      "js": ["bettergithub.js"],
      "css" : ["styles.css"]
    }
  ]

}
